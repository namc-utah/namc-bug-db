{
  "extends": "../../tsconfig-react.json",
  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "./src"
  ]
}
